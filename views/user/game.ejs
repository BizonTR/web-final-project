<div class="border-end bg-white" id="sidebar-wrapper">
    <div class="sidebar-heading border-bottom bg-light">Oyunlar</div>
    <div class="list-group list-group-flush">
        
        <% data.forEach((element,index) => { var cl="alert alert-success"; %>
          
            <% if ((index+1)%3==1) { cl="alert alert-success"}%>
            <% if ((index+1)%3==2) { cl="alert alert-info"}%>
            <% if ((index+1)%3==0) { cl="alert alert-warning"}%>          <div class="<%=cl %> mt-1 mb-0">
            <a href="/user/game/view/<%=element.url%>" class="link-primary" style="text-decoration: none;"><strong><%=element.title %></strong></a>
        </div>
        <% }) %>
    </div>
</div>